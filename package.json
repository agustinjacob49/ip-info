  "dependencies": {
    "awilix": "^7.0.3",
    "awilix-express": "^7.0.0",
    "aws-sdk": "^2.1214.0",
    "axios": "^0.27.2",
    "currency-symbol-map": "^5.1.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.3",
    "@shelf/jest-dynamodb": "^3.3.1",
